              "to": "to"
            }
          ]
        },
        {
          "type": "trim",
          "name": "list",
          "conditions": [
            {
              "type": "between",
              "from": "the",
              "to": "list"
            },
            {
              "type": "between",
              "from": "my",
              "to": "list"
            }
          ]
        }
      ]
